<h1>Create A Super Hero</h1>
<ul>
  <%@superhero.errors.full_messages.each do |message|%>
  <li><%=message%></li>
</ul>
<%end%>



<%=form_for @superhero do |f|%>
  <%=f.label :name%>
  <%=f.text_field :name%> <br>

  <%=f.label :super_name%>
  <%=f.text_field :super_name%> <br>

  <%=f.collection_select :superpower, Superpower.all, :id, :name%>
  <%=f.submit "Create Super Hero"%>
<%end%>
