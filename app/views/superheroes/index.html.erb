<h2>All Superheroes</h2>

    <%= form_tag({:controller => "superheroes", :action => "index"}, method: :get) do %>
         <%= text_field_tag :query, params[:query], {:class =>"search-query", :placeholder=>"Search"}  %>
         submit_tag("Send", name: 'query[submit]')
        <% end %>
  <% @superheroes.each do |superhero| %>
    <li>
      <%= link_to superhero.super_name, superhero_path(superhero) %>
    </li>
    <br>
  <% end %>
</ul>
